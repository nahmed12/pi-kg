{
    "nodes": [
        {
            "node": "Diseases",
            "example": [
                "Diabetes mellitus",
                "Malaria"
            ],
            "display": [
                {
                    "label": "Diseases ID",
                    "text": "${id}",
                    "type": "text"
                },
                {
                    "label": "label",
                    "text": "${label}",
                    "type": "text"
                }
            ],
            "relation": [],
            "search": [
                "id",
                "label"
            ]
        },
        {
            "node": "Gene",
            "example": [
                "COPB2",
                "ABCG2"
            ],
            "display": [
                {
                    "label": "Gene ID",
                    "text": "${id}",
                    "type": "text"
                },
                {
                    "label": "label",
                    "text": "${label}",
                    "type": "text"
                },
                {
                    "label": "uri",
                    "text": "${uri}",
                    "href": "${uri}",
                    "type": "link"
                }
            ],
            "relation": [],
            "search": [
                "id",
                "label"
            ]
        },
        {
            "node": "Principal Investigator",
            "example": [
                "MU A",
                "KJERSTI MARIE AAGAARD"
            ],
            "display": [
                {
                    "label": "Principal Investigator ID",
                    "text": "${id}",
                    "type": "text"
                },
                {
                    "label": "label",
                    "text": "${label}",
                    "type": "text"
                },
                {
                    "label": "organization",
                    "text": "${organization}",
                    "type": "text"
                },
                {
                    "label": "Number_Papers_Published",
                    "text": "${Number_Papers_Published}",
                    "type": "text"
                },
                {
                    "label": "First_publication_year",
                    "text": "${First_publication_year}",
                    "type": "text"
                },
                {
                    "label": "Rank",
                    "text": "${Rank}",
                    "type": "text"
                },
                {
                    "label": "Total_citations",
                    "text": "${Total_citations}",
                    "type": "text"
                },
                {
                    "label": "Top_subfield",
                    "text": "${Top_subfield}",
                    "type": "text"
                },
                {
                    "label": "h_index",
                    "text": "${h_index}",
                    "type": "text"
                },
                {
                    "label": "Composite_score",
                    "text": "${Composite_score}",
                    "type": "text"
                },
                {
                    "label": "hm_index",
                    "text": "${hm_index}",
                    "type": "text"
                }
            ],
            "relation": [],
            "search": [
                "id",
                "label"
            ]
        }
    ],
    "edges": [
        {
            "match": [
                "PI Gene"
            ],
            "selected": true,
            "display": [
                {
                    "label": "source",
                    "text": "${source}",
                    "type": "text"
                },
                {
                    "label": "relation",
                    "text": "${relation}",
                    "type": "text"
                },
                {
                    "label": "target",
                    "text": "${target}",
                    "type": "text"
                },
                {
                    "label": "source label",
                    "text": "${source_label}",
                    "type": "text"
                },
                {
                    "label": "target label",
                    "text": "${target_label}",
                    "type": "text"
                }
            ]
        },
        {
            "match": [
                "PI Diseases"
            ],
            "selected": true,
            "display": [
                {
                    "label": "source",
                    "text": "${source}",
                    "type": "text"
                },
                {
                    "label": "relation",
                    "text": "${relation}",
                    "type": "text"
                },
                {
                    "label": "target",
                    "text": "${target}",
                    "type": "text"
                },
                {
                    "label": "source label",
                    "text": "${source_label}",
                    "type": "text"
                },
                {
                    "label": "target label",
                    "text": "${target_label}",
                    "type": "text"
                },
                {
                    "label": "jaccard similarity",
                    "text": "${jaccard_similarity}",
                    "type": "text"
                }
            ]
        }
    ],
    "header": {
        "title": "NIDDK RESEARCHER KG",
        "icon": {
            "src": "https://minio.dev.maayanlab.cloud/enrichr-kg/enrichr-kg.png",
            "favicon": "https://minio.dev.maayanlab.cloud/enrichr-kg/enrichr-kg.png",
            "alt": "Enrichr logo",
            "key": "enrichr_logo",
            "width": 100,
            "height": 60
        },
        "tabs": [
            {
                "endpoint": "/",
                "label": "Term & Gene Search",
                "type": "page",
                "component": "KnowledgeGraph",
                "props": {
                    "init_function": "initialize_kg"
                }
            }
        ]
    },
    "footer": [
        {
            "type": "github",
            "code": "https://github.com/MaayanLab/enrichr-kg",
            "issues": "https://github.com/MaayanLab/enrichr-kg/issues"
        },
        {
            "type": "icon",
            "key": "lab_icon",
            "src": "https://minio.dev.maayanlab.cloud/enrichr-kg/maayan-lab.jpg",
            "alt": "MaayanLab",
            "href": "https://labs.icahn.mssm.edu/maayanlab/",
            "height": 125,
            "width": 150
        },
        {
            "type": "icon",
            "key": "center_icon",
            "src": "https://minio.dev.maayanlab.cloud/enrichr-kg/ISMMS_Center_Bioinformatics_RGB_Vrtl.jpg",
            "alt": "COB",
            "href": "https://icahn.mssm.edu/research/bioinformatics",
            "height": 125,
            "width": 200
        }
    ]
}
